    <li data-target="#myCarousel" data-slide-to="2"></li>
  </ol>

  <!-- Wrapper for slides -->
  <div class="carousel-inner">
    <div class="item active" style="height:490px;">
      <img src="https://scontent.fphx1-2.fna.fbcdn.net/v/t31.0-8/18237980_10209287354885726_2128376433509158023_o.jpg?oh=7f16e29f9c7602e23d212709742fe608&oe=5B4D6DBF" alt="Lisa's 80s Catholic High School Movie">
